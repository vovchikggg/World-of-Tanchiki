class BotsList
{
    field Bots data;
    field BotsList next;

    constructor BotsList new(Bot car, List cdr)
    {
        let data = car;
        let next = cdr;
        return this;
    }

    method Bots getData()
    {
        return data;
    }

    method BotsList getNext()
    {
        return next;
    }

    method void dispose(){
        do Memory.deAlloc(this);
        return;
    }
}