class Brick
{
    field int x;
    field int y;
    field bool alive;

    constructor Brick new(int _x, int _y)
    {
        let x = _x;
        let y = _y;
        let alive = true;
        return this;
    }

    method int getX(){
        return x;
    }

    method int getY(){
        return y;
    }

    method bool getAlive(){
        return alive;
    }

    method void dispose()
    {
        do Memory.deAAlloc(this);
        return;
    }
}